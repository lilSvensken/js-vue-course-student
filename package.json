{
    "name": "js-and-vue-course",
    "version": "1.0.0",
    "description": "JS and Vue course",
    "scripts": {
        "test": "jest --reporters=default --reporters=jest-junit -t",
        "test-i": "jest -o",
        "test-watch": "jest --watch -t",
        "watch": "webpack -w --mode development",
        "build": "webpack --mode production",
        "update": "git fetch origin main && git merge origin/main"
    },
    "devDependencies": {
        "@babel/plugin-transform-modules-commonjs": "^7.15.4",
        "@vue/vue2-jest": "^27.0.0-alpha.2",
        "axios": "^0.27.2",
        "babel-jest": "^27.0.0",
        "babel-loader": "^8.2.5",
        "babel-polyfill": "^6.26.0",
        "babel-preset-env": "^1.7.0",
        "css-loader": "^6.7.1",
        "eslint": "^8.28.0",
        "file-loader": "^6.2.0",
        "jest": "^27.0.0",
        "jest-cli": "^27.0.0",
        "jest-fetch-mock": "^3.0.3",
        "jest-junit": "^14.0.0",
        "jest-serializer-vue": "^2.0.2",
        "path": "^0.12.7",
        "sinon": "^14.0.0",
        "vue-loader": "^15.8.3",
        "vue-style-loader": "^4.1.3",
        "vue-template-compiler": "^2.7.8",
        "webpack": "^5.74.0",
        "webpack-cli": "^4.10.0",
        "webpack-encoding-plugin": "^0.3.1"
    },
    "dependencies": {
        "@babel/preset-env": "^7.18.9",
        "@testing-library/dom": "^8.19.0",
        "@vue/test-utils": "^1.2.2",
        "melanke-watchjs": "^1.5.2",
        "moment": "^2.29.4",
        "moment-locales-webpack-plugin": "^1.2.0",
        "vue": "^2.6.11",
        "vuex": "^3.6.2",
        "yarn": "^1.22.19"
    },
    "license": "ISC",
    "jest": {
        "testEnvironment": "node",
        "reporters": [
            "default",
            "jest-junit"
        ],
        "snapshotSerializers": [
            "jest-serializer-vue"
        ],
        "transformIgnorePatterns": [
            "/node_modules/(?!@vue/test-utils)"
        ],
        "transform": {
            "^.+\\.vue$": "@vue/vue2-jest",
            "^.+\\.js$": "babel-jest"
        }
    }
}
